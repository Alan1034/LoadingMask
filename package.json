  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue2": "^2.3.3",
    "@vitejs/plugin-vue2-jsx": "^1.1.1",
    "chalk": "^5.4.1",
    "cross-env": "^7.0.3",
    "less": "^4.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.89.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-image-optimizer": "^2.0.2"
  },
